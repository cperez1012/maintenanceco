<!DOCTYPE html>
<html>
  <head>
    <title>Maintenaceco</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
      <%= render 'partials/jumbotron' %>
      <%= render "partials/alerts" %>

      <%if logged_in?%>
      <%= render "partials/nav" %>
      <%end%>
      
      <main role="main">
      <div class="container">
    <%= yield %>
      <hr>
      </div>
  </body>
  <%= render 'partials/footer' %>
</html>
