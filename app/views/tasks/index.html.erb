<h1>Welcome to MaintenanceCo, <%=@current_user.username %>!</h1><br><br>

  <h1>Lists of Tasks</h1>

  <table>
  <tr>
    <th>Project</th>
    <th>Priority</th>
    <th>Status</th>
    <th>Name</th>
    <th>Summary</th>
    <th>Description</th>
  </tr>
 
  <% @tasks.each do |task| %>
    <tr>
      <td><%= task.project %></td>
      <td><%= task.priority %></td>
      <td><%= task.status %></td>
      <td><%= task.name %></td>
      <td><%= task.summary %></td>
      <td><%= task.description %></td>
    </tr>
  <% end %>
</table>

  <h4> <%= link_to("Create Task", new_task_path, method: :get) %></h4><br>

  <h4> <%= link_to("Edit Task", edit_task_path(:id)) %></h4><br>

</div>
