<div class='container-fluid'>
<p id="notice"><%= notice %></p>
<div class='row py-3 px-3'>
  <div class='col-12 col-sm-8'>
    <h3 class=''>Tenants</h3>
    <table id='tenant-table' class="table table-hover">
      <thead  class="thead-light">
        <tr>
          <th>List Number</th>
          <th>Tenant Name</th>
          <th>Phone Number</th>
          <th>Email</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @tenants.each do |tenant| %>
          <tr>
            <td><%= tenant.id %></td>
            <td><%= link_to tenant.renter, tenant_path(tenant)%></td>
            <td><%= tenant.renter_cell_phone %> </td>
            <td><%= tenant.renter_email%></td>
            # <td><%= link_to 'Show', employee %></td>
            # <td><%= link_to 'Edit', edit_tenant_path(@tenant) %></td>
            # <td><%= link_to 'Delete', tenant, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
    <br>
  </div>

  <div class='col-12 col-sm-4'>
    <h3>New Employee</h3>
    <%= render 'form', employee: @employee %>
  </div>
</div>
</div>